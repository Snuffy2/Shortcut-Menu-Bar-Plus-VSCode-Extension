{
  "name": "shortcut-menu-bar-plus",
  "displayName": "Shortcut Menu Bar Plus",
  "description": "Add handy buttons like beautify, show opened files, save, toggle terminal, activity bar etc to editor menu bar. User-defined buttons are also possible.",
  "version": "3.1.0",
  "publisher": "snuffy2",
  "author": {
    "name": "Snuffy2"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "license": "GPL-3.0-or-later",
  "icon": "images/menubar-plus.png",
  "galleryBanner": {
    "color": "#752d00",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/Snuffy2/Shortcut-Menu-Bar-Plus-VSCode-Extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Snuffy2/Shortcut-Menu-Bar-Plus-VSCode-Extension.git"
  },
  "homepage": "https://github.com/Snuffy2/Shortcut-Menu-Bar-Plus-VSCode-Extension",
  "categories": [
    "Programming Languages",
    "Other",
    "Visualization"
  ],
  "keywords": [
    "shortcut",
    "toolbar",
    "shortcuts",
    "menu",
    "menubar",
    "tool",
    "status",
    "editor",
    "bar",
    "shortcut menu bar",
    "shortcut menu bar plus",
    "button"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
