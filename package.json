          },
          "ShortcutMenuBarPlus.beautify": {
            "title": "Beautify selection/file",
            "type": "boolean",
            "default": true,
            "description": "show icon for 'Beautify/Format selection or file'"
          },
          "ShortcutMenuBarPlus.toggleRenderWhitespace": {
