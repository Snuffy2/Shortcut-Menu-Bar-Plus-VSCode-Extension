    "menus": {
      "editor/title": [
        {
          "when": "editorTextFocus && config.ShortcutMenuBarPlus.switchHeaderSource",
          "command": "ShortcutMenuBarPlus.switchHeaderSource",
          "group": "navigation@1"
        },
        {
          "when": "config.ShortcutMenuBarPlus.navigateBack",
          "command": "ShortcutMenuBarPlus.navigateBack",
          "group": "navigation@2"
        },
        {
          "when": "config.ShortcutMenuBarPlus.navigateForward",
          "command": "ShortcutMenuBarPlus.navigateForward",
          "group": "navigation@3"
        },
        {
          "when": "!isInDiffEditor && !markdownPreviewFocus && config.ShortcutMenuBarPlus.save",
          "command": "ShortcutMenuBarPlus.save",
          "group": "navigation@4"
        },
        {
          "when": "!isInDiffEditor && !markdownPreviewFocus && config.ShortcutMenuBarPlus.beautify",
          "command": "ShortcutMenuBarPlus.beautify",
          "group": "navigation@5"
        },
        {
          "when": "config.ShortcutMenuBarPlus.toggleRenderWhitespace",
          "command": "ShortcutMenuBarPlus.toggleRenderWhitespace",
          "group": "navigation@6"
        },
        {
          "when": "!isInDiffEditor && !markdownPreviewFocus && config.ShortcutMenuBarPlus.openFilesList",
          "command": "ShortcutMenuBarPlus.openFilesList",
          "group": "navigation@7"
        },
        {
          "when": "config.ShortcutMenuBarPlus.toggleTerminal",
          "command": "ShortcutMenuBarPlus.toggleTerminal",
          "group": "navigation@8"
        },
        {
          "when": "config.ShortcutMenuBarPlus.toggleActivityBar",
          "command": "ShortcutMenuBarPlus.toggleActivityBar",
          "group": "navigation@9"
        },
        {
          "when": "config.ShortcutMenuBarPlus.quickOpen",
          "command": "ShortcutMenuBarPlus.quickOpen",
          "group": "navigation@10"
        },
        {
          "when": "config.ShortcutMenuBarPlus.findReplace",
          "command": "ShortcutMenuBarPlus.findReplace",
          "group": "navigation@11"
        },
        {
          "when": "textInputFocus && !editorReadonly && config.ShortcutMenuBarPlus.undo",
          "command": "ShortcutMenuBarPlus.undo",
          "group": "navigation@12"
        },
        {
          "when": "textInputFocus && !editorReadonly && config.ShortcutMenuBarPlus.redo",
          "command": "ShortcutMenuBarPlus.redo",
          "group": "navigation@13"
        },
        {
          "when": "editorTextFocus && !editorReadonly && config.ShortcutMenuBarPlus.commentLine",
          "command": "ShortcutMenuBarPlus.commentLine",
          "group": "navigation@14"
        },
        {
